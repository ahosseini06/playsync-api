{
  "kind": "collectionType",
  "collectionName": "teams",
  "info": {
    "singularName": "team",
    "pluralName": "teams",
    "displayName": "Team",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "coaches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::coach.coach",
      "mappedBy": "team"
    },
    "matches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::match.match",
      "mappedBy": "team_2"
    },
    "rankings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ranking.ranking",
      "mappedBy": "team"
    },
    "players": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::player.player",
      "mappedBy": "teams"
    },
    "checked_in": {
      "type": "boolean",
      "default": false
    },
    "wins_in_current_pool": {
      "type": "integer"
    },
    "role_can_register": {
      "type": "enumeration",
      "enum": [
        "player",
        "coach"
      ],
      "default": "player"
    }
  }
}
