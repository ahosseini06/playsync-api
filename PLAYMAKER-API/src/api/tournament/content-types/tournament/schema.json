{
  "kind": "collectionType",
  "collectionName": "tournaments",
  "info": {
    "singularName": "tournament",
    "pluralName": "tournaments",
    "displayName": "Tournament",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "scheduled",
        "active",
        "complete"
      ],
      "default": "scheduled"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "tournaments"
    },
    "num_tiers": {
      "type": "integer"
    },
    "pools": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pool.pool",
      "mappedBy": "tournament"
    },
    "matches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::match.match",
      "mappedBy": "tournament"
    },
    "rankings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ranking.ranking",
      "mappedBy": "tournament"
    },
    "venues": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::venue.venue",
      "mappedBy": "tournament"
    },
    "bracket_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::bracket-type.bracket-type"
    },
    "pool_play": {
      "type": "boolean"
    },
    "cross_over": {
      "type": "boolean"
    },
    "stage": {
      "type": "enumeration",
      "enum": [
        "registering",
        "configuration",
        "check-in",
        "pool-play",
        "play-offs",
        "complete"
      ],
      "default": "registering"
    },
    "event_dates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event-date.event-date",
      "mappedBy": "tournament"
    },
    "public_id": {
      "type": "uid",
      "targetField": "name"
    },
    "private_id": {
      "type": "string"
    },
    "pool_size": {
      "type": "integer"
    },
    "teams": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::team.team"
    }
  }
}
